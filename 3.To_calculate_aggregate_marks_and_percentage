#include<iostream>
using namespace std;

int main(){
    
     int arr[5];
     int sum=0;
     
     for(int i=0;i<5;i++){
         cout<<"Enter the marks of "<<i+1<<"subject"<<endl;
         cin>>arr[i];
         sum+=arr[i];
     }
     
     cout<<"Aggregate Marks ="<<sum/5.0<<endl;
     
     cout<<"Percentage = "<<(sum/500.0)*100<<"%"<<endl;
     
     
}
