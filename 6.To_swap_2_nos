#include<iostream>
using namespace std;

void swap(int *a,int *b){
    int temp;
    temp = *a;
    *a = *b;
    *b = temp;
}

int main(){
    int c,d;
     cout<<"Enter first number:"<<endl;
     cin>>c;
     
     cout<<"Enter second number:"<<endl;
     cin>>d;
     
     
     cout<<"The number before swap are "<<c<<" "<<d<<endl;
     swap(&c,&d);
     
     cout<<"The number after swap are "<<c<<" "<<d<<endl;
}
