#include<iostream>
using namespace std;

int main(){
    
    int a,b,c;
    cout<<"Enter the three numbers:"<<endl;
    cin>>a>>b>>c;
    
    int max =0;
    
    if(max<a){
        max =a;
        if(max<b){
            max =b;
            if(max <c){
                max =c;
            }
        }
        else if(max<c){
            max = c;
        }
    }
    
    cout<<"Hence the greatest of the three numbers is :"<<max;
    
    
} 
