
include<iostream>
using namespace std;

void swap(int *a,int *b){
    int temp;
    temp = *a;
    *a = *b;
    *b = temp;
}

int main(){
    
    int n;
    int sum =0;
    
    cout<<"Enter the five digit number:"<<endl;
    cin>>n;
    
    while(n>0){
        sum+= n%10;
        n=n/10;
    }
    
    cout<<sum<<endl;
}
